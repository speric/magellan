<% form_for @venue, :url => venues_path do |f| %>
	<%= f.error_messages %>
	<p>
		<%= f.label :name %>
		<%= f.text_field :name, :size => 35 %><br />
	</p>
	
	<p>
		<%= f.label :address %>
		<%= f.text_field :address, :size => 35 %><br />
	</p>
	
	<%= f.hidden_field :user_id, {:value => @user.id} %>
	<%= submit_tag "Create" %>
<% end %>